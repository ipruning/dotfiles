[
  {
    "context": "EmptyPane || SharedScreen",
    "bindings": {
      "space g g": [
        "workspace::SendKeystrokes",
        ": new center terminal enter lazygit enter"
      ]
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-r": "projects::OpenRecent"
    }
  },
  {
    "context": "Editor && vim_mode == normal && vim_operator == none && !VimWaiting",
    "bindings": {
      "ctrl-r": "projects::OpenRecent",
      "space g g": [
        "workspace::SendKeystrokes",
        ": new center terminal enter lazygit enter"
      ],
      "space f t": "task::Spawn",
      "space f f": "pane::DeploySearch",
      "space e": "workspace::ToggleLeftDock",
      "space t": "workspace::ToggleBottomDock",
      "space p": "projects::OpenRecent",
      "space /": [
        "editor::ToggleComments",
        {
          "advance_downwards": false
        }
      ],
      "alt-space": "workspace::ToggleLeftDock"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "cmd-n": "workspace::NewTerminal",
      "cmd-d": "pane::CloseActiveItem",
      "escape": "workspace::ToggleBottomDock"
    }
  },
  {
    "context": "AssistantPanel",
    "bindings": {}
  }
]
